<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="RadarChartView">
    <attr name="startColor" format="color" />
    <attr name="endColor" format="color" />
    <attr name="axisColor" format="color" />
    <attr name="graphColor" format="color" />
    <attr name="parts" format="integer" />
    <attr name="axisMax" format="float" />
    <attr name="axisTick" format="float" />
    <attr name="axisWidth" format="float" />
    <attr name="graphWidth" format="float" />
    <attr name="graphStyle" format="enum">
      <enum name="fill" value="0" />
      <enum name="stroke" value="1" />
    </attr>
    <attr name="textSize" format="dimension" />
    <attr name="circlesOnly" format="boolean" />
    <attr name="autoSize" format="boolean" />
    <attr name="smoothGradient" format="boolean" />
  </declare-styleable>
</resources>